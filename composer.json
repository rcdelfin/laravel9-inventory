{
    "name": "rcdelfin/inventory",
    "description": "An Inventory Management Package for products, (Laravel 9, php8.0 & php8.1)",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Raymund C. Delfin",
            "email": "rcdexe@gmail.com"
        }
    ],
    "keywords": [
        "Inventory",
        "Product",
        "Laravel"
    ],
    "homepage": "https://github.com/rcdelfin/laravel9-inventory",
    "require": {
        "php": "^8.0|^8.1",
        "stancl/tenancy": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Rcdelfin\\Inventory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rcdelfin\\Inventory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rcdelfin\\Inventory\\ProductServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --filter 'Rcdelfin\\Inventory\\Tests"
        ]
    },
    "scripts-descriptions": {
        "test": "Run Unit Testing"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
